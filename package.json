{
  "name": "upperhand",
  "version": "0.0.1",
  "description": "囲碁に似た雰囲気の陣とりゲーム",
  "private": true,
  "scripts": {
    "build:html": "pug -o dist -P src/html/page/",
    "build:css": "stylus src/css/index.styl -o dist/upperhand-0.0.1.css",
    "build:js": "webpack --mode development --devtool inline-source-map",
    "build": "npm run build:html && npm run build:css && npm run build:js",
    "release": "npm run build:html && npm run build:css && webpack --mode production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kobalab/UpperHand.git"
  },
  "keywords": [],
  "author": "Satoshi Kobayashi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kobalab/UpperHand/issues"
  },
  "homepage": "https://github.com/kobalab/UpperHand#readme",
  "overrides": {
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "pug-cli": "^1.0.0-alpha6",
    "stylus": "^0.59.0",
    "webpack-cli": "^5.0.1"
  }
}
